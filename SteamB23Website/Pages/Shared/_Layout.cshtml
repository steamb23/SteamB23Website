<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SteamB23Website</title>
    <link href="~/css/normalize.css" type="text/css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/site.min.css" type="text/css" rel="stylesheet" asp-append-version="true" />
    <link href="//cdn.jsdelivr.net/npm/font-kopubworld@1.0" rel="stylesheet" />
    <link href="~/fontawesome/css/all.css" rel="stylesheet">
</head>
<body class="preload">

    <main>
        @RenderBody()
    </main>
    
    <script src="https://cdn.jsdelivr.net/npm/@@popperjs/core@@2.11.5/dist/umd/popper.min.js" integrity="sha384-Xe+8cL9oJa6tN/veChSP7q+mnSPaj5Bcu9mPX5F5xIGE0DVittaqT5lorf0EI7Vk" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.min.js" integrity="sha384-kjU+l4N0Yf4ZOJErLsIcvOU2qSb74wXpOhqTvwVx3OElZRweTnQ6d31fXEoRD1Jy" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script>
        var scroll = new SmoothScroll('a[href*="#"]');
    </script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>